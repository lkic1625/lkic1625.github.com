<span class="kt">int</span> <span class="nf">bipartiteMatch</span><span class="p">()</span> <span class="p">{</span>
	<span class="c1">//ì²˜ìŒì—ëŠ” ì •ì ì´ ëª¨ë‘ ì—°ê²°ë˜ì–´ ìˆì§€ ì•ŠìŒ.</span>
	<span class="n">aMatch</span> <span class="o">=</span> <span class="n">vector</span><span class="o">&lt;</span><span class="kt">int</span><span class="o">&gt;</span><span class="p">(</span><span class="n">n</span><span class="p">,</span> <span class="o">-</span><span class="mi">1</span><span class="p">);</span>
	<span class="n">bMatch</span> <span class="o">=</span> <span class="n">vector</span><span class="o">&lt;</span><span class="kt">int</span><span class="o">&gt;</span><span class="p">(</span><span class="n">m</span><span class="p">,</span> <span class="o">-</span><span class="mi">1</span><span class="p">);</span>
	<span class="kt">int</span> <span class="n">size</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
